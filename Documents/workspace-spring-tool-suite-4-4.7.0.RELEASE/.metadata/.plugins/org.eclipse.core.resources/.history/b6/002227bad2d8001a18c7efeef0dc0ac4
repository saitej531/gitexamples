package studentdemo;

public class Student {
	
	
	private String roll number;
	
	private int age;

	public String getRoll() {
		return roll;
	}

	public void setRoll(String roll) {
		this.roll = roll;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	


	
	
	
	
	
	

}
