package studentdemo;

import java.util.*;


public class Student {
	
	
	private String rollno;
	
	private int age;
	
	public void setRollno(String rollno) {
		this.rollno=rollno;
	}

	public String getRoll() {
		return rollno;
	}

	public void setRoll(String roll) {
		this.rollno = roll;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	


	
	
	
	
	
	

}
